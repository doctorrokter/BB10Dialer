<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:width="90dp"
        android:height="90dp">
        <shape android:shape="oval" android:innerRadius="45dp">
            <solid android:color="#282828"/>
            <stroke android:color="#779933" android:width="2dp"/>
        </shape>
    </item>

    <item
        android:width="45dp"
        android:height="90dp"
        android:left="45dp">
        <shape
            android:shape="rectangle">

            <solid android:color="#282828"/>
        </shape>
    </item>


</layer-list>