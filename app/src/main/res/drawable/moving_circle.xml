<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:height="90dp"
        android:width="90dp">

        <shape
            android:shape="oval"
            android:innerRadius="45dp">
            <stroke
                android:width="4dp" android:color="#969696"/>

            <solid android:color="#282828"/>
        </shape>
    </item>

    <item
        android:width="70dp"
        android:height="70dp"
        android:left="10dp"
        android:top="10dp">

        <shape
            android:shape="oval">

            <solid android:color="#969696"/>
        </shape>
    </item>
</layer-list>